        expected $true
    }
    {
        condition #present
        parameters [ id 3 ]
        expected $false
    }
    {
        condition #present
        parameters [ identifier 3 ]
        expected $true
    }
    {
        condition #present
        parameters [ #keyword -8 ]
        expected $false
    }
    {
        condition #present
        parameters [ #keyword -5 ]
        expected $true
    }

